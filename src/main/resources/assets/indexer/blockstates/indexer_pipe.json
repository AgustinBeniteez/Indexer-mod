{
  "multipart": [
    { "apply": { "model": "indexer:block/indexer_pipe_core" } },
    { "when": { "north": "true" }, "apply": { "model": "indexer:block/indexer_pipe_attachment", "uvlock": true } },
    { "when": { "east": "true" }, "apply": { "model": "indexer:block/indexer_pipe_attachment", "y": 90, "uvlock": true } },
    { "when": { "south": "true" }, "apply": { "model": "indexer:block/indexer_pipe_attachment", "y": 180, "uvlock": true } },
    { "when": { "west": "true" }, "apply": { "model": "indexer:block/indexer_pipe_attachment", "y": 270, "uvlock": true } },
    { "when": { "up": "true" }, "apply": { "model": "indexer:block/indexer_pipe_attachment", "x": 270, "uvlock": true } },
    { "when": { "down": "true" }, "apply": { "model": "indexer:block/indexer_pipe_attachment", "x": 90, "uvlock": true } }
  ]
}